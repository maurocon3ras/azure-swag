{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputDocument",
      "databaseName": "swademo",
      "collectionName": "quotes",
      "connectionStringSetting": "swademo_DOCUMENTDB",
      "id": "{Query.id}",
      "partitionKey": "{Query.partitionKeyValue}"
    }
  ]
}
